<%- include('../partials/header.ejs')%>

    <h1 id="newAccountTitle">Welcome to the signup page
    </h1>
    <form action='/users' method='POST'>
        <input type="text" name="firstName" id="firstName" class="firstLog" placeholder="Enter your first name"
            required>
        <input type="text" name="lastName" id="lastName" placeholder="Enter your last name" class="lastLog" required>
        <input type="email" id="email" name="email" placeholder="Enter your email address" class="newEmailLog" required>
        <input type="password" id="password" name="password" placeholder="Enter your password" class="newPasswordLog"
            required>

        <input type="submit" value="Login" id="newLoginSubmit">
    </form>

    <%- include('../partials/footer.ejs')%>